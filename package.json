{
  "name": "dht_sensor_exporter",
  "version": "0.0.1",
  "description": "prometheus exporter for dht sensor information",
  "main": "index.js",
  "author": "Alex Vernacchia",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "SENSOR_TESTING=true node index.js",
    "build": "pkg -c package.json index.js"
  },
  "dependencies": {
    "express": "^4.18.1",
    "node-dht-sensor": "^0.4.3",
    "prom-client": "^14.1.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "pkg": "^5.8.0"
  },
  "resolutions": {
    "pkg/pkg-fetch": "^3.2"
  },
  "engines": {
    "node": "^14"
  },
  "pkg": {
    "targets": [
      "node14-linux-x64", 
      "node14-linux-arm64",
      "node14-linuxstatic-armv7"
    ],
    "outputPath": "build"
  }
}
